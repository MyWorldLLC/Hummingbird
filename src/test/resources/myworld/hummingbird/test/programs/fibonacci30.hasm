.symbols
    fibonacci function $fibonacci
            long
            parameters 3
            registers 7

.code
  # fib(30)
  CONST r1, 30
  CONST r2, 1
  CONST r3, 2
  CALL r4, $fibonacci, r1, 3
  RETURN r4
  fibonacci:
    # Params (r0 = n, r1 = 1, r2 = 2)
    IFGE r0, r2, $recurse
    RETURN r0
  recurse:
    #fib(n - 1)
    SUB r0, r0, r1 # r1 = n - 1
    CALL r3, $fibonacci, r0, 3

    #fib(n - 2)
    SUB r0, r0, r1 # r1 = n - 2
    CALL r4, $fibonacci, r0, 3

    ADD r0, r3, r4
    RETURN r0