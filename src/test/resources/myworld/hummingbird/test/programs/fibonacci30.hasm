.symbols
    fibonacci function $fibonacci
            int
            parameters 1

.code
  # fib(30)
  CONST ri1, 30
  CALL ri0, 0
  RETURN ri0
  fibonacci:
    PARAM ri4, 1
    CONST ri2, 2
    IFGE ri4, ri2, $recurse
    RETURN ri4
  recurse:
    # Constants (ri4 = n)
    CONST ri2, 1
    CONST ri3, 2

    #fib(n - 1)
    SUB ri1, ri4, ri2 # ri1 = n - 1
    CALL ri5, 0

    #fib(n - 2)
    SUB ri1, ri4, ri3 # ri1 = n - 2
    CALL ri0, 0

    ADD ri0, ri0, ri5
    RETURN ri0