.symbols
    callOneMillion function $callOneMillion
        int
        parameters 0

.code
    CONST ri2, 1000000
    CONST ri1, 1
    CONST ri3, 0
  loop:
    CALL $callOneMillion
    ADD ri3, ri3, ri0
    IFLT ri3, ri2, $loop
    COPY ri0, ri3
    RETURN
  callOneMillion:
    COPY ri0, ri1
    RETURN
