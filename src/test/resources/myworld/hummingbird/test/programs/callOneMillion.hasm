.symbols
    callOneMillion function $callOneMillion
        int
        parameters 0
        registers 6

.code
    CONST ri2, 1000000
    CONST ri3, 0
  loop:
    CALL ri0, %callOneMillion
    ADD ri3, ri3, ri0
    IFLT ri3, ri2, $loop
    COPY ri0, ri3
    RETURN ri0
  callOneMillion:
    CONST ri0, 1
    RETURN ri0
