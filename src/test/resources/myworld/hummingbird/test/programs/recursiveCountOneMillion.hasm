.symbols
    countToAMillion function $countToAMillion
        int
        parameters 1
        registers 3

.code
  CONST ri0, 0
  CALL ri0, 0, 0, 0, 0, 0, 0, 0
  RETURN ri0
  countToAMillion:
    PARAM ri0, 0
    CONST ri1, 1
    ADD ri0, ri1
    CONST ri2, 1000000
    IFLT ri0, ri2, $recurse
    RETURN ri0
  recurse:
    CALL ri0, 0, 0, 0, 0, 0, 0, 0
    RETURN ri0